{
    "manifest_version": 2,
    "name": "Century Box",
    "description": "manage all your tab with century box",
    "version": "1.0.0",
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js https://code.jquery.com/jquery-latest.min.js https://www.google-analytics.com/analytics.js https://code.jquery.com/jquery-3.2.1.slim.min.js https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js; object-src 'self'",
    "browser_action": { 
      "default_icon": {
          "32": "icons/32.png"
        }
    },
    "icons": {
      "16":"icons/16.png",
      "32": "icons/32.png",
      "48":"icons/48.png",
      "128":"icons/128.png"
      },
    "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
    "permissions": [
      "activeTab",
      "tabs",
      "clipboardWrite"
    ] 
  }
  